package com.fp.modelo;

public class Ayuda_cuantia {
	private int ayuda;

	public Ayuda_cuantia(int ayuda) {
		super();
		this.ayuda = ayuda;
	}

	public int getAyuda() {
		return ayuda;
	}

	public void setAyuda(int ayuda) {
		this.ayuda = ayuda;
	}

	@Override
	public String toString() {
		return "Ayuda_cuantia [ayuda=" + ayuda + "]";
	}

}
