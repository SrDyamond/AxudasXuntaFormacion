package com.fp.modelo;

import java.sql.Date;

public class Datos {
	private String dni;
	private String nombre;
	private Date fecha_nacimiento;
	private double renta_actual;
	private int numero_mienbros_familiares;

	public Datos(String dni, String nombre, Date fecha_nacimiento, double renta_actual,
			int numero_mienbros_familiares) {
		super();
		this.dni = dni;
		this.nombre = nombre;
		this.fecha_nacimiento = fecha_nacimiento;
		this.renta_actual = renta_actual;
		this.numero_mienbros_familiares = numero_mienbros_familiares;
	}

	public String getDni() {
		return dni;
	}

	public void setDni(String dni) {
		this.dni = dni;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public Date getFecha_nacimiento() {
		return fecha_nacimiento;
	}

	public void setFecha_nacimiento(Date fecha_nacimiento) {
		this.fecha_nacimiento = fecha_nacimiento;
	}

	public double getRenta_actual() {
		return renta_actual;
	}

	public void setRenta_actual(double renta_actual) {
		this.renta_actual = renta_actual;
	}

	public int getNumero_mienbros_familiares() {
		return numero_mienbros_familiares;
	}

	public void setNumero_mienbros_familiares(int numero_mienbros_familiares) {
		this.numero_mienbros_familiares = numero_mienbros_familiares;
	}

	@Override
	public String toString() {
		return "Datos_solicitante [dni=" + dni + ", nombre=" + nombre + ", fecha_nacimiento=" + fecha_nacimiento
				+ ", renta_actual=" + renta_actual + ", numero_mienbros_familiares=" + numero_mienbros_familiares + "]";
	}

}
